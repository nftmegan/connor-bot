# Connor Bot System Environment Template
# Copy to .env in each service directory

# Telegram Bot
BOT_TOKEN=your_bot_token_here

# Database
DATABASE_URL=file:./bot.db

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Application
NODE_ENV=development
LOG_LEVEL=info

# Scraper
HEADLESS=true
SCRAPER_INTERVAL_MINUTES=10

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:3001
